{
  "name": "handterm-backend",
  "version": "1.0.0",
  "description": "Backend for HandTerm application",
  "license": "Apache-2.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "cpCdk": "cp ../handterm/src/shared/endpoints.ts lambda/cdkshared/",
    "build": "tsc",
    "predeploy": "yarn install && yarn build",
    "deploy": "yarn cpCdk && cdk deploy --profile mark --region us-east-1",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.643.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.637.0",
    "@aws-sdk/client-s3": "^3.425.0",
    "@aws-sdk/client-ssm": "^3.0.0",
    "@octokit/rest": "^21.0.2",
    "@types/aws-lambda": "^8.10.140",
    "aws-cdk-lib": "2.148.1",
    "aws-lambda": "^1.0.7",
    "aws-jwt-verify": "^4.0.1",
    "constructs": "^10.0.0",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@aws-sdk/client-ssm": "^3.0.0",
    "@types/aws-lambda": "^8.10.140",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "20.14.2",
    "aws-cdk": "2.148.1",
    "aws-sdk-client-mock": "^4.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5"
  }
}
